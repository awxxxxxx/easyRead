{
    "manifest_version":2,
    "name":"easyRead",
    "version":"1.0",
    "description":"划词搜索制定的书在豆瓣上的评分",
    "icons":{
        "16":"images/16.png",
        "48":"images/48.png",
        "128":"images/128.png"
    },
    "browser_action":{
        "default_icon":{
            "19":"images/19.png",
            "38":"images/38.png"
        },
        "default_title":"买买买"
    },
    "content_scripts":[
        {
            "matches":["http://*","https://*"],
            "css":["css/easyRead.css"],
            "js":["js/easyRead.js"]
        }
    ]
}
